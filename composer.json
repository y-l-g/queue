{
  "name": "pogo/queue",
  "description": "FrankenPHP In-Memory Queue Driver for Laravel",
  "require": {
    "php": "^8.5",
    "laravel/framework": "^12.44.0",
    "laravel/octane": "^2.13.3"
  },
  "require-dev": {
    "orchestra/testbench": "^10.8.0",
    "phpunit/phpunit": "^12.5.4"
  },
  "autoload": {
    "psr-4": {
      "Pogo\\Queue\\": "lib/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pogo\\Queue\\Tests\\": "lib/tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Pogo\\Queue\\PogoQueueServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
